{
  "name": "mercado-futuro",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "env $(cat .env) docker-compose up -d",
    "dev:start": "nodemon src/server.js",
    "dev:queue": "nodemon src/queue.js",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "pretest": "env $(cat .env.test) docker-compose -f docker-compose.test.yml -p fastfeet-server-test up -d && cross-env NODE_ENV=test sequelize db:migrate",
    "test": "cross-env NODE_ENV=test jest --forceExit",
    "posttest": "cross-env NODE_ENV=test sequelize db:migrate:undo:all && env $(cat .env.test) docker-compose -f docker-compose.test.yml -p fastfeet-server-test down",
    "securetest": "cross-env SECURE=true test",
    "resetDatabase": "sequelize db:migrate:undo:all; sequelize db:migrate; sequelize db:seed:all",
    "migrate": "env $(cat .env) sequelize db:migrate",
    "seed": "env $(cat .env) sequelize db:seed:all"
  },
  "dependencies": {
    "@sentry/node": "^5.15.4",
    "aws-sdk": "^2.692.0",
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.2.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "date-fns": "^2.0.0-beta.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-handlebars": "^3.1.0",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.22.0",
    "ioredis": "^4.16.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemailer": "^6.4.2",
    "nodemailer-express-handlebars": "^3.1.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "rate-limit-redis": "^1.7.0",
    "redis": "^3.0.2",
    "sequelize": "^5.21.11",
    "shelljs": "^0.8.3",
    "uuid": "^7.0.3",
    "youch": "^2.0.10",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/express-brute": "^0.0.37",
    "@types/express-rate-limit": "^5.0.0",
    "@types/ioredis": "^4.14.9",
    "@types/jest": "^25.2.0",
    "@types/mongoose": "^5.7.8",
    "@types/rate-limit-redis": "^1.7.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "jest": "^25.2.7",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "sucrase": "^3.12.1",
    "supertest": "^4.0.2"
  }
}
